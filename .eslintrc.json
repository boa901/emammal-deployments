{
    "env": {
        "es2021": true,
        "node": true
    },
    "settings": {
        "import/resolver": {
            "alias": {
                "map": [
                    [
                        "#express",
                        "./express"
                    ]
                ],
                "extensions": [
                    ".js",
                    ".jsx",
                    ".ts",
                    ".tsx"
                ]
            }
        }
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    }
}
